local function QGMPPFL_script() -- TrollAudio.LocalScript 
	local script = Instance.new('LocalScript', TrollAudio)

	local enabled = false
	local PlayAllSounds = function()
		for _, sound in next, game:GetDescendants() do
			if sound:IsA("Sound") then
				sound:Play()
			end
		end
	end
	
	script.Parent.MouseButton1Click:Connect(function()	
		if enabled == false then
			enabled = true
	
			PlayAllSounds()
		else
			enabled = false
	
			for _, sound in next, game:GetDescendants() do
				if sound:IsA("Sound") then
					sound.Playing = false
				end
			end
		end
	end)
end
