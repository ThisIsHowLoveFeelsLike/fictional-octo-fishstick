local function XSQGECJ_script() -- WhatIsTheNextMap.LocalScript 
	local script = Instance.new('LocalScript', WhatIsTheNextMap)

	local nameMap = {
		Asylum = "Asylum",
		hotel = "Hotel",
		Factory = "Factory",
		Tarnish = "Tarnish",
		School = "School",
		MadhouseLab = "Madhouse Lab",
		building = "Complex",
		ruins = "Ancient Ruins",
		Cargo = "Cargo Hold",
		Mall = "Abandoned Mall",
		b = "Ocean Liner"
	}
	
	local map = workspace:FindFirstChild("MAP")
	
	if map then
		while task.wait(.1) do
			for _, v in ipairs(map:GetChildren()) do
				if v:IsA("Model") and nameMap[v.Name] then
					script.Parent.Text = "Current Map: " .. nameMap[v.Name]
				end
			end
		end
	end
end
